# https://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[CHANGES]
indent_size = 4
indent_style = space

[configure]
indent_size = 3
indent_style = space

[**.xml]
indent_size = 2
indent_style = space
